[{
  "selector": "core",
  "style": {
    "selection-box-color": "#AAD8FF",
    "selection-box-opacity": "0.5"
  }
}, {
  "selector": "node[type=\"selected\"]",
  "style": {
    "width": "25",
    "height": "25",
    "content": "data(label)",
    "font-size": "12px",
    "text-valign": "center",
    "text-halign": "center",
    "background-color": "#a55",
    "color": "#fff",
    "z-index": "10"
  }
}, {
  "selector": "node[type=\"neighbor\"]",
  "style": {
    "width": "25",
    "height": "25",
    "content": "data(label)",
    "font-size": "8px",
    "text-valign": "center",
    "text-halign": "center",
    "background-color": "#444",
    "color": "#aaa",
    "z-index": "10"
  }
}, {
  "selector": "node[?query]",
  "style": {
    "background-clip": "none",
    "background-fit": "contain"
  }
}, {
  "selector": "node:selected",
  "style": {
    "background-color": "#75c"
  }
}, {
  "selector": "edge",
  "style": {
    "curve-style": "bezier",
    "haystack-radius": "0.5",
    "opacity": "0.5",
    "width": "1",
    "target-arrow-shape": "triangle"
  }
}, {
  "selector": "node.unhighlighted",
  "style": {
    "opacity": "0.2"
  }
}, {
  "selector": "edge.unhighlighted",
  "style": {
    "opacity": "0.05"
  }
}, {
  "selector": ".highlighted",
  "style": {
    "z-index": "999999"
  }
}, {
  "selector": "node.highlighted",
  "style": {
    "background-color": "#A3f"
  }
}, {
  "selector": "edge.filtered",
  "style": {
    "opacity": "0"
  }
}, {
  "selector": "edge[group=\"coexp\"]",
  "style": {
    "line-color": "#d0b7d5"
  }
}, {
  "selector": "edge[group=\"coloc\"]",
  "style": {
    "line-color": "#a0b3dc"
  }
}, {
  "selector": "edge[group=\"gi\"]",
  "style": {
    "line-color": "#90e190"
  }
}, {
  "selector": "edge[group=\"path\"]",
  "style": {
    "line-color": "#9bd8de"
  }
}, {
  "selector": "edge[group=\"pi\"]",
  "style": {
    "line-color": "#eaa2a2"
  }
}, {
  "selector": "edge[group=\"predict\"]",
  "style": {
    "line-color": "#f6c384"
  }
}, {
  "selector": "edge[group=\"spd\"]",
  "style": {
    "line-color": "#dad4a2"
  }
}, {
  "selector": "edge[group=\"spd_attr\"]",
  "style": {
    "line-color": "#D0D0D0"
  }
}, {
  "selector": "edge[group=\"reg\"]",
  "style": {
    "line-color": "#D0D0D0"
  }
}, {
  "selector": "edge[group=\"reg_attr\"]",
  "style": {
    "line-color": "#D0D0D0"
  }
}, {
  "selector": "edge[group=\"user\"]",
  "style": {
    "line-color": "#f0ec86"
  }
}]
