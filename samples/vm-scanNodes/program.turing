.data:
INDICES    $indices
OUTPUT     $out

.alias:
ALIAS %out,     r0
ALIAS %indices, r2

.text:
LOAD %indices, $indices
LOAD %out,     $out

ALLOC_ENTITY_IDS r1

ADD_COL %out, r1, 0

SCAN_NODES r100, {r1}
    r100_START:
    JEND   r100, r100_END
    UPDATE r100

    WRITE %out

    GOTO r100_START
r100_END:

EXIT
