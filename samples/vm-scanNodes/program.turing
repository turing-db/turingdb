.data:
INDICES    $indices
OUTPUT     $out

.alias:
ALIAS %out,     r0
ALIAS %indices, r2

.text:
LOAD %indices, $indices
LOAD %out,     $out

ALLOC_ENTITY_IDS r1

ADD_COL %out, r1, _

SCAN_NODES i0, %indices, {r1}
    i0_START:
    JEND   i0, i0_END
    UPDATE i0

    WRITE %out

    GOTO i0_START
i0_END:

EXIT
