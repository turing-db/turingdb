/// QUERY
MATCH (n:Person { name: "Remy" })
DELETE n

/// RESULT
flowchart TD
    0["`
        __SCAN_NODES__
    `"]
    1["`
        __VAR__
        __name__: n
    `"]
    2["`
        __FILTER_NODE__
        __label__: Person
        __has predicate__
        __dep__ _n_._name_ (String)
    `"]
    3["`
        __GET_PROPERTY__
        __var__ n
        __prop__ name
    `"]
    4["`
        __WRITE__
        __delete_node__ n
    `"]
    0-->3
    1-->4
