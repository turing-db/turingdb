/// QUERY
MATCH (n:Person)
WHERE n.name = "Luc"
CREATE (n)-[e:KNOWS_WELL]->(m:Person) RETURN n;

/// RESULT
flowchart TD
    0["`
        __SCAN_NODES__
    `"]
    1["`
        __VAR__
        __name__: n
    `"]
    2["`
        __FILTER_NODE__
        __label__: Person
        __has predicate__
        __dep__ _n_._name_ (String)
    `"]
    3["`
        __GET_PROPERTY__
        __var__ n
        __prop__ name
    `"]
    4["`
        __WRITE__
        __node__ (0:Person)
        __edge__ (n)-[0:KNOWS_WELL]->(0)
    `"]
    5["`
        __PRODUCE_RESULTS__
    `"]
    0-->3
