/// QUERY
MATCH (a { name: "Remy" })-[e:KNOWS_WELL]-(m { name: "Adam" })
WHERE a:Person AND m:Person AND e.duration > 10 
RETURN a;

/// RESULT
flowchart TD
    0["`
        __SCAN_NODES__
    `"]
    1["`
        __VAR__
        __name__: a
    `"]
    2["`
        __FILTER_NODE__
        __label__: Person
        __prop__ _a_._name_
    `"]
    3["`
        __GET_EDGES__
    `"]
    4["`
        __VAR__
        __name__: e
    `"]
    5["`
        __FILTER_EDGE__
        __edge_type__: KNOWS_WELL
        __has predicate__
        __dep__ _e_._duration_
    `"]
    6["`
        __GET_EDGE_TARGET__
    `"]
    7["`
        __VAR__
        __name__: m
    `"]
    8["`
        __FILTER_NODE__
        __label__: Person
        __prop__ _m_._name_
    `"]
    9["`
        __PRODUCE_RESULTS__
    `"]
