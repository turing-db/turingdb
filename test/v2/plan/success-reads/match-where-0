/// QUERY
MATCH (a), (b) WHERE a.name = b.name  RETURN a, b

/// RESULT
flowchart TD
    0["`
        __SCAN_NODES__
    `"]
    1["`
        __VAR__
        __name__: a
    `"]
    2["`
        __FILTER_NODE__
        __has predicate__
        __dep__ _a_._name_
        __dep__ _b_._name_
    `"]
    3["`
        __SCAN_NODES__
    `"]
    4["`
        __VAR__
        __name__: b
    `"]
    5["`
        __FILTER_NODE__
    `"]
    6["`
        __GET_PROPERTY__
        __var__ a
        __prop__ name
    `"]
    7["`
        __GET_PROPERTY__
        __var__ b
        __prop__ name
    `"]
    8["`
        __CARTESIAN_PRODUCT__
    `"]
    9["`
        __PRODUCE_RESULTS__
    `"]
